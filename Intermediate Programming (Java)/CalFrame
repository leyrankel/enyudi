    }                                        

    private void jButtonMinusActionPerformed(java.awt.event.ActionEvent evt) {                                             
        value = "";
        operation = '-';
        num1 = Double.parseDouble(keloutput.getText());
    }                                            

    private void jButtonZeroActionPerformed(java.awt.event.ActionEvent evt) {                                            
        value += '0';
        keloutput.setText(value);
    }                                           

    private void jButtonDotActionPerformed(java.awt.event.ActionEvent evt) {                                           
        value += '.';
        keloutput.setText(value);
    }                                          

    private void jButtonEqualActionPerformed(java.awt.event.ActionEvent evt) {                                             
        num2 = Double.parseDouble(keloutput.getText());
            switch (operation) {
                case '+':
                    ans = num1 + num2;
                    break;
                case '-':
                    ans = num1 - num2;
                    break;
                case '*':
                    ans = num1 * num2;
                    break;
                case '/':
                    if (num2 != 0) {
                        ans = num1 / num2;
                    } else {
                        keloutput.setText("Error");
                        return;
                    }
                    break;
            }
            keloutput.setText(String.valueOf(ans));
            num1 = ans;
    }                                            

    private void jButtonPlusActionPerformed(java.awt.event.ActionEvent evt) {                                            
        value = "";
        operation = '+';
        num1 = Double.parseDouble(keloutput.getText());
        keloutput.setText(value);
    }                                           


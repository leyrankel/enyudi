        keloutput.setText(value);
    }                                        

    private void jButtonSlashActionPerformed(java.awt.event.ActionEvent evt) {                                             
        value = "";
        keloutput.setText(value);
        operation = '/';
        num1 = Double.parseDouble(keloutput.getText());
    }                                            
    
    private void jButtonCActionPerformed(java.awt.event.ActionEvent evt) {                                         
        value = "";
        keloutput.setText(value);
    }                                        

